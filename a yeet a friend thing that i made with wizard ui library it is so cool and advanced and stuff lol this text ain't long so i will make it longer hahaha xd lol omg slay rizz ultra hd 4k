local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()

local PhantomForcesWindow = Library:NewWindow("YaF")

local KillingCheats = PhantomForcesWindow:NewSection("D3STR0YER")

local PortCheats = PhantomForcesWindow:NewSection("Portable Stuff")

local FarmingCheats = PhantomForcesWindow:NewSection("AutoStuff")

local PaidForFreeCheats = PhantomForcesWindow:NewSection("P4F")

KillingCheats:CreateButton("Land Destroyer (sometimes works, forever)", function()

while wait() do
   game:GetService("ReplicatedStorage").Remote.Throw.Server.Landed:InvokeServer(9926290.241311073,3)
end

end)

KillingCheats:CreateButton("Throw Destroyer (sometimes works, forever)", function()

while wait() do
   game:GetService("ReplicatedStorage").Remote.Throw.Server.Request:FireServer()
end

end)

KillingCheats:CreateToggle("Land Destroyer (toggle)", function(land)

_G.Land = land

if _G.Land then
while wait() do
   game:GetService("ReplicatedStorage").Remote.Throw.Server.Landed:InvokeServer(9926290.241311073,3)
end

end)

KillingCheats:CreateToggle("Throw Destroyer (toggle)", function(throw)

_G.Throw = throw

if _G.Throw then
while wait() do
   game:GetService("ReplicatedStorage").Remote.Throw.Server.Request:FireServer()
end

end)

PortCheats:CreateTextbox("Hatch Egg (WIP)", function(text)

print(text)

end)

FarmingCheats:CreateToggle("Auto Throw (they alr got this)", function(autothrow)

_G.AutoThrow = autothrow

if _G.AutoThrow then
while wait() do
   game:GetService("ReplicatedStorage").Remote.Throw.Server.Request:FireServer()
end

end)

FarmingCheats:CreateToggle("Auto Reward (i think)", function(autoreward)

_G.AutoReward = autoreward

if _G.AutoReward then
wait(0.4)
   game:GetService("ReplicatedStorage").Remote.Player.Server.ClaimTimedReward:InvokeServer("DailyReward")
end

end)

PaidForFreeCheats:CreateToggle("Auto Rebirth", function(autorebirth)

_G.AutoRebirth = autorebirth

if _G.AutoRebirth then
wait(0.4)
   game:GetService("ReplicatedStorage").Remote.Player.Server.AttemptRebirth:InvokeServer()
end

end)
